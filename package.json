{
  "name": "quote-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": " >=14.16.1"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/main.js",
    "clean": "rm -rf dist",
    "clean:static": "rm -rf static",
    "clean:all": "npm run clean && npm run clean:static",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "build:client": "npm run clean:static && cd client && npm run build && mkdir ../static && cp -R build/* ../static",
    "prod": "pm2 start ./dist/main.js",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "core-js": "^3.11.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "quotesy": "^1.0.5",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.2"
  }
}
